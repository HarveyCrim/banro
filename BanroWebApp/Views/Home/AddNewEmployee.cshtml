@model BanroWebApp.Models.t_beneficiaires
@{
    Layout = "~/Views/Shared/_Menu.cshtml";
    ViewBag.Title = "ViewEmployed";
    BanroWebApp.Models.Authenticate auth = null;
    if (Session["userinfo"] != null)
    {
        auth = (BanroWebApp.Models.Authenticate)Session["userinfo"];
    }
    else
    {
        Response.Redirect("~/");
    }

}
<div id="page-wrapper" data-ng-app="app" ng-controller="CtrlHome" style="display:inline-block;width:80%">
    <div class="row">
        <div class="col-lg-12">
            <h3 class="page-header">
                View beneficiairies <i class="fa fa-users" style="float:right;"></i>
            </h3>
        </div>
        <!-- /.col-lg-12 -->
    </div>

    <div class="col-lg-12" >
        <div class="panel panel-default">
            <div class="panel-heading">
                @if (auth.Priority.Equals("administrator") || auth.Priority.Equals("user"))
            {
                    <label>
                        Add a new employee
                    </label>
                  


                    <button class="btn btn-success" id="btnUpdateEmployed" style="float:right;margin-top:-.5em;margin-left:10px;" ng-show="isBtnVisible" ng-click="udpateEmployed()"><i class="fa fa-pencil"></i> Modify</button>
                    <button class="btn btn-success" id="btnInsertEmployed" style="float:right;margin-top:-.5em;margin-left:10px;" ng-click="addEmployed()" ng-hide="isBtnVisible"><i class="fa fa-plus"></i> Register</button>
                    <button class="btn btn-primary" data-toggle="modal" data-target="#myModalSearch" style="float:right;margin-top:-.5em;" ng-click=""><i class="fa fa-search"></i> Search</button>
                    <div class="form-group" style="float:right;margin-top:-.5em;margin-left:10px;">
                        @{
                            if (auth.Priority.ToLower().Equals("administrator"))
                            {
                                <select class="form-control" id="cboastatus">

                                    <option value="1">Enabled</option>
                                    <option value="0">Disabled</option>
                                </select>
                            }
                        }

                    </div>
                            }
                            else
                            {
                                <label>
                                    Search Employee
                                </label>
                                <button class="btn btn-primary" data-toggle="modal" data-target="#myModalSearch" style="float:right;margin-top:-.5em;" ng-click=""><i class="fa fa-search"></i> Search</button>

                            }



            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="alert alert-warning" id="alertWarnning" style="display:none;text-align:center;">
                        Lease wait.....
                    </div>
                    <div class="alert alert-success" id="alertSuccess" style="display:none;text-align:center;">
                        Successfull registration
                    </div>
                    <div class="alert alert-success" id="alertSuccess2" ng-show="isModify" style="text-align:center;">
                        Successfull Modify
                    </div>
                    <img id="pictureEmployed" src="data:image/png;base64," alt="Alternate Text" style="width:150px;height:150px;border:1px solid silver;" />

                    <div style="display:inline-block;margin-top:1em;margin-left:1em;">
                        @*<button type="button" class="btn btn-primary btn-circle btn-lg">
                                <i class="fa fa-camera"></i>
                            </button>*@

                        <button type="button" ng-click="browserPicture()" id="picture_employee" class="btn btn-success btn-circle btn-lg">
                            <i class="fa fa-image"></i>
                        </button>


                    </div>



                    <hr />
                    <div class="col-lg-6">



                        <div class="form-group">
                            <label>Beneficiary's Name</label>
                            @Html.TextBoxFor(Model => Model.C_name, new { Class = "form-control", data_ng_model = "nameBene" })
                        </div>
                        <div class="form-group">
                            <label>Phone number</label>
                            @Html.TextBoxFor(Model => Model.C_name, new { Class = "form-control", data_ng_model = "phoneBene" })
                        </div>
                        <div class="form-group">
                            <label>Gender</label>
                            <select ng-model="sexBene" class="form-control">
                                <option></option>
                                <option value="M">Male</option>
                                <option value="F">Female</option>
                            </select>
                        </div>
                        <div class="form-group">
                            <label>Marital Status</label>
                            <select class="form-control" id="cboEtatCivil" ng-model="cboEtatCivil" ng-change="EtatCivil()">

                                <option value="single">single</option>
                                <option value="married">married</option>
                                <option value="divorced">divorced</option>
                                <option value="widow(er)">widow(er)</option>
                            </select>
                        </div>
                        <div class="row" style="">


                            <div class="col-lg-6" style="width:100%;">
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        Family <button class="btn btn-primary" data-toggle="modal" data-target="#myModalChild" style="float:right;margin-top:-.5em;" id="btnChild" ng-click="ClearChild()"><i class="fa fa-plus"></i> Add</button>
                                        <button class="btn btn-primary" data-toggle="modal" data-target="#myModalChild" style="float:right;margin-top:-.5em;display:none;" id="btnChild2"><i class="fa fa-plus"></i> Ajouter</button>

                                    </div>
                                    <!-- /.panel-heading -->
                                    <div class="panel-body">
                                        <div class="table-responsive">
                                            <table class="table" style="font-size:12px;">
                                                <thead>
                                                    <tr>
                                                        <th>Child's Name</th>
                                                        <th>Born on</th>
                                                        <th>Gender</th>
                                                        <th>Status</th>
                                                        <th>Edition</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="children in tabChild">
                                                        <td>{{children.name}}</td>
                                                        <td>{{children.datenais}}</td>
                                                        <td>{{children.sex}}</td>
                                                        <td>{{children.status}}</td>
                                                        <td disabled><a href="#/employed" ng-hide="linkDisabled" ng-click="editChild(children)">edit</a></td>

                                                    </tr>

                                                </tbody>
                                            </table>
                                        </div>
                                        <!-- /.table-responsive -->
                                    </div>
                                    <!-- /.panel-body -->
                                </div>
                                <!-- /.panel -->
                            </div>
                            <!-- /.col-lg-6 -->
                        </div>
                        <!-- /.row -->



                    </div>
                    <div class="col-lg-6" style="float:right;">
                        <div class="form-group">
                            <label>Beneficiary's Id Number</label>
                            <input type="text" class="form-control" ng-model="idnumberBene" id="tMat" />

                        </div>
                        @if (!auth.Priority.Equals("user"))
                        {


                            <div class="form-group">
                                <label>Company's name</label>
                                <select class="form-control" ng-model="cbo_succ" id="cbo_succ" ng-change="getDepartementBySucc('Employed')">
                                    <option ng-repeat="succ in ListSuccursales">{{succ.name}}</option>
                                </select>
                            </div>
                        }
                        else
                        {

                            <div class="form-group" style="display:none;">
                                <label>Succursale</label>
                                <input type="text" id="cbo_succ" class="form-control" ng-model="cbo_succ" />
                            </div>
                        }
                        <div class="form-group">
                            <label>Department</label>
                            <select class="form-control" ng-model="cbo_depart" id="cbo_departEmployed" ng-change="beneficiaireDepartement('Employed')">
                                <option ng-repeat="dep in ListDepartementBySucc">{{dep.code_depart}}</option>
                            </select>
                        </div>

                        <div class="form-group">
                            <label>Born On</label>
                            <table style="width:100%">
                                <tr>
                                    <td>
                                        <select class="form-control" ng-model="cboday" id="cboday">
                                            <option ng-repeat="day in arrayDay">
                                                {{day}}
                                            </option>

                                        </select>
                                    </td>
                                    <td>
                                        <select ng-model="cbomonth" class="form-control">
                                            <option value="01">January</option>
                                            <option value="02">February</option>
                                            <option value="03">March</option>
                                            <option value="04">April</option>
                                            <option value="05">May</option>
                                            <option value="06">June</option>
                                            <option value="07">July</option>
                                            <option value="08">August</option>
                                            <option value="09">September</option>
                                            <option value="10">October</option>
                                            <option value="11">November</option>
                                            <option value="12">December</option>

                                        </select>
                                    </td>
                                    <td>
                                        <select ng-model="cboyear" id="cboyear" class="form-control">
                                            <option ng-repeat="year in arrayYear">
                                                {{year}}
                                            </option>
                                        </select>
                                    </td>
                                </tr>
                            </table>

                        </div>


                        <div class="row" id="table_partner" style="">


                            <div class="col-lg-6" style="width:100%;">
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        Partner
                                        <button class="btn btn-primary" data-toggle="modal" data-target="#myModalSearchConjoint" id="btnaddSearchPartner" style="float:right;margin-top:-.5em;display:none;"><i class="fa fa-search"></i> Search</button>
                                        <button class="btn btn-primary" data-toggle="modal" data-target="#myModalPartner" id="btnaddPartner" style="float:right;margin-top:-.5em;margin-right:0.5em;" ng-show="btnAddPartner" ng-click="cleanConjoint()"><i class="fa fa-plus"></i> Add</button>
                                        <button class="btn btn-primary" data-toggle="modal" data-target="#myModalPartner" id="btnaddPartner2" style="float:right;margin-top:-.5em;margin-right:0.5em;display:none;"><i class="fa fa-plus"></i> Ajouter</button>
                                    </div>
                                    <!-- /.panel-heading -->
                                    <div class="panel-body">
                                        <div class="table-responsive">
                                            <table class="table" style="font-size:12px;">
                                                <thead>
                                                    <tr>
                                                        <th>Partner's Name</th>
                                                        <th>Gender</th>
                                                        <th>Phone number</th>
                                                        <th>Account Status</th>
                                                        <th>Edition</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="partner in tabPartner">
                                                        <td>{{partner.name}}</td>
                                                        <td>{{partner.sex}}</td>
                                                        <td>{{partner.phone}}</td>
                                                        <td>{{partner.account_system}}</td>
                                                        <td><a href="#/employed" ng-hide="linkDisabled" ng-click="editPartner(partner)">edit</a></td>

                                                    </tr>

                                                </tbody>
                                            </table>
                                        </div>
                                        <!-- /.table-responsive -->
                                    </div>
                                    <!-- /.panel-body -->
                                </div>
                                <!-- /.panel -->
                            </div>
                            <!-- /.col-lg-6 -->
                        </div>
                        <!-- /.row -->
                    </div>
                    <!-- /.col-lg-6 (nested) -->
                    <!-- /.col-lg-6 (nested) -->
                </div>
                <!-- /.row (nested) -->
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>

    <div class="modal fade" id="myModalChild" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="background-color: #337ab7;color:white;">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title" id="myModalLabel">{{titleChild}}</h4>
                </div>

                <div class="modal-body">
                    <form role="form" method="post">
                        <img id="pict_child" src="data:image/png;base64," alt="Alternate Text" style="width:150px;height:150px;border:1px solid silver;" />
                        <div style="display:inline-block;margin-top:1em;margin-left:1em;">
                            @*<button type="button" class="btn btn-primary btn-circle btn-lg">
                                    <i class="fa fa-camera"></i>
                                </button>*@

                            <button type="button" ng-click="browserPictureChild()" class="btn btn-success btn-circle btn-lg">
                                <i class="fa fa-image"></i>
                            </button>

                        </div>
                        <hr />
                        <div class="form-group">
                            <label>Child's Name</label>
                            @Html.TextBoxFor(Model => Model.C_id, new { Class = "form-control", @Value = @ViewBag.Id_Succ, @Id = "id_succ", data_ng_model = "nameChild" })
                        </div>
                        <div class="form-group">
                            @*<label>Born On</label>
                                @Html.TextBoxFor(Model => Model.C_id, new { Class = "form-control", @Value = @ViewBag.Id_Succ, @Id = "id_succ", data_ng_model = "dateChild",type="text" })*@
                            <label>Born On</label>
                            <table style="width:100%">
                                <tr>
                                    <td>
                                        <select class="form-control" ng-model="cbodayChild" id="cbodayChild">
                                            <option ng-repeat="day in arrayDay">
                                                {{day}}
                                            </option>

                                        </select>
                                    </td>
                                    <td>
                                        <select ng-model="cbomonthChild" id="cbomonthChild" class="form-control">
                                            <option> </option>
                                            <option value="01">January</option>
                                            <option value="02">February</option>
                                            <option value="03">March</option>
                                            <option value="04">April</option>
                                            <option value="05">May</option>
                                            <option value="06">June</option>
                                            <option value="07">July</option>
                                            <option value="08">August</option>
                                            <option value="09">September</option>
                                            <option value="10">October</option>
                                            <option value="11">November</option>
                                            <option value="12">December</option>

                                        </select>
                                    </td>
                                    <td>
                                        <select ng-model="cboyearChild" id="cboyearChild" class="form-control">
                                            <option ng-repeat="year in arrayYearChild">
                                                {{year}}
                                            </option>
                                        </select>
                                    </td>
                                </tr>
                            </table>
                        </div>
                        <div class="form-group">
                            <label>Gender</label>
                            <select ng-model="sexChild" class="form-control">
                                <option></option>
                                <option value="M">Male</option>
                                <option value="F">Female</option>
                            </select>

                        </div>
                        <div class="form-group">
                            <label>Status by Old</label>
                            <select class="form-control" ng-model="statusChild">
                                <option ng-repeat="status in listStatus">{{status}}</option>

                            </select>
                        </div>
                        @{
                            if (auth.Priority.ToLower().Equals("administrator"))
                            {
                                <div class="form-group" style="display:none;">
                                    <label>Account Status</label>
                                    <select id="cboastatusChild" class="form-control">
                                        <option value="1">Enabled</option>
                                        <option value="0">Disabled</option>
                                    </select>
                                </div>
                            }
                        }







                    </form>
                </div>
                <div class="modal-footer">
                    <div class="alert alert-success" id="alertSucc" ng-show="{{isVisible}}" style="display:none;float:left;width:70px;background-color:white;">
                        Successfull Modify
                    </div>

                    <button type="button" ng-click="AddChild('new')" class="btn btn-primary"><i class="fa fa-plus"></i>Save and Create</button>
                    <button type="button" ng-click="AddChild('close')" class="btn btn-primary"><i class="fa fa-plus"></i>Save</button>
                    <button type="button" id="btnFermer" class="btn btn-default" data-dismiss="modal">Cancel</button>
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
    <!-- /.modal -->



    <div class="modal fade" id="myModalPartner" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="background-color: #337ab7;color:white;">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title" id="myModalLabel">{{titleConjoint}}</h4>
                </div>

                <div class="modal-body">
                    <form role="form" method="post">
                        <img id="picturePartner" src="data:image/png;base64," alt="Alternate Text" style="width:150px;height:150px;border:1px solid silver;" />
                        <div style="display:inline-block;margin-top:1em;margin-left:1em;">
                            <button type="button" class="btn btn-primary btn-circle btn-lg">
                                <i class="fa fa-camera"></i>
                            </button>

                            <button type="button" ng-click="browserPicturePartner()" class="btn btn-success btn-circle btn-lg">
                                <i class="fa fa-image"></i>
                            </button>

                        </div>
                        <hr />

                        <div class="form-group" id="controlChoice">
                            <label>Employee ?</label>
                            <div class="radio">
                                <label>
                                    <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" ng-click="controlsSucc('yes')">Oui
                                </label>
                            </div>
                            <div class="radio">
                                <label>
                                    <input type="radio" name="optionsRadios" id="optionsRadios2" value="option1" ng-click="controlsSucc('no')" checked>non
                                </label>
                            </div>
                        </div>
                        <hr />
                        <div class="form-group">
                            <label>Partner's Name</label>
                            @Html.TextBoxFor(Model => Model.C_id, new { Class = "form-control", @Value = @ViewBag.Id_Succ, @Id = "id_succ", data_ng_model = "namePartner" })
                        </div>
                        <div class="form-group">
                            <label>Gender</label>
                            <select class="form-control">
                                <option></option>
                                <option value="M">Male</option>
                                <option value="F">Female</option>
                            </select>

                        </div>
                        <div class="form-group">
                            <label>Phone number</label>
                            @Html.TextBoxFor(Model => Model.C_id, new { Class = "form-control", @Value = @ViewBag.Id_Succ, @Id = "id_succ", data_ng_model = "phonePartner" })
                        </div>
                        <div class="form-group">
                            @*<label>Born On</label>
                                @Html.TextBoxFor(Model => Model.C_id, new { Class = "form-control", @Value = @ViewBag.Id_Succ, @Id = "id_succ", data_ng_model = "datenaisPartner",type="text" })*@
                            <label>Born On</label>
                            <table style="width:100%">
                                <tr>
                                    <td>
                                        <select class="form-control" ng-model="cbodaySpouse" id="cbodaySpouse">
                                            <option ng-repeat="day in arrayDay">
                                                {{day}}
                                            </option>

                                        </select>
                                    </td>
                                    <td>
                                        <select ng-model="cbomonthSpouse" id="cbomonthSpouse" class="form-control">
                                            <option value="01">January</option>
                                            <option value="02">February</option>
                                            <option value="03">March</option>
                                            <option value="04">April</option>
                                            <option value="05">May</option>
                                            <option value="06">June</option>
                                            <option value="07">July</option>
                                            <option value="08">August</option>
                                            <option value="09">September</option>
                                            <option value="10">October</option>
                                            <option value="11">November</option>
                                            <option value="12">December</option>

                                        </select>
                                    </td>
                                    <td>
                                        <select ng-model="cboyearSpouse" id="cboyearSpouse" class="form-control">
                                            <option ng-repeat="year in arrayYear">
                                                {{year}}
                                            </option>
                                        </select>
                                    </td>
                                </tr>
                            </table>
                        </div>
                        @{
                            if (!auth.Priority.Equals("user"))
                            {
                                <div class="form-group" id="group-mat" style="display:none;">
                                    <label>Id Number Partner</label>
                                    <input type="text" ng-model="idnumberSpouse" class="form-control" />
                                </div>

                                <div class="form-group" id="group-succ" style="display:none;">
                                    <label>List of Companies</label>
                                    <select class="form-control" ng-model="cbo_succPart" id="cbo_succPart" ng-change="getDepartementBySucc('Partner')">
                                        <option ng-repeat="succ in ListSuccursales">{{succ.name}}</option>
                                    </select>
                                </div>

                                <div class="form-group" id="group-depart" style="display:none;">
                                    <label>List of Departments</label>
                                    <select class="form-control" ng-model="cbo_departPart" id="cbo_departPart" ng-change="beneficiaireDepartement('Partner')">
                                        <option ng-repeat="dep in ListDepartementBySucc2">{{dep.code_depart}}</option>
                                    </select>
                                </div>
                            }
                            else
                            {
                                <div class="form-group" id="group-succ" style="display:none;">
                                    <label>Id Number Partner</label>
                                    <input type="text" class="form-control" ng-model="idnumberSpouse" />
                                </div>

                                <div class="form-group" id="group-succ">
                                    <label>Succursale</label>
                                    <input type="text" class="form-control" ng-model="cbo_succPart2" id="cbo_succPart" />
                                </div>

                                <div class="form-group" id="group-depart" style="display:none;">
                                    <label>Departement</label>
                                    <select class="form-control" ng-model="cbo_departPart" id="cbo_departPart" ng-change="beneficiaireDepartement('Partner')">
                                        <option ng-repeat="dep in ListDepartementBySucc2">{{dep.code_depart}}</option>
                                    </select>
                                </div>

                            }
                        }
                        @{
                            if (auth.Priority.ToLower().Equals("administrator"))
                            {
                                <div class="form-group" style="display:none;">
                                    <label>Account Status</label>
                                    <select id="cboastatusPartner" class="form-control">
                                        <option value="1">Enabled</option>
                                        <option value="0">Disabled</option>
                                    </select>
                                </div>
                            }
                        }





                    </form>
                </div>
                <div class="modal-footer">
                    <div class="alert alert-success" id="alertSucc" style="display:none;float:left;width:70px;background-color:white;">
                        Successfull Modify
                    </div>
                    <button type="button" class="btn btn-primary" ng-click="EditConjoint()" id="btnClose" ng-show="!visibleAddPartner">Modify</button>
                    <button type="button" class="btn btn-primary" style="display:none;" data-dismiss="modal" id="btnClosePartner" ng-show="!visibleAddPartner">Modify</button>
                    <button type="button" ng-click="AddPartner()" id="" class="btn btn-primary" ng-show="visibleAddPartner"><i class="fa fa-plus"></i>Add</button>
                </div>
            </div>
            <!-- /.modal-content -->
        </div>
        <!-- /.modal-dialog -->
    </div>
    <!-- /.modal -->

    <div class="modal fade" id="myModalSearch" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="background-color: #337ab7;color:white;">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title" id="myModalLabel">Search employee</h4>
                </div>

                <div class="modal-body">
                    <div class="form-group input-group">
                        <input type="text" class="form-control" ng-model="nameEmployed" placeholder="Write employee's">
                        <span class="input-group-btn">
                            <button class="btn btn-primary" type="button" ng-click="seachEmployed('Master')" style="height:35px;">
                                <i class="fa fa-search"></i>
                            </button>
                        </span>
                    </div>
                    <hr />
                    <div class="row">
                        <div class="col-lg-6" style="width:100%;">
                            <div class="panel panel-default">

                                <!-- /.panel-heading -->
                                <div class="panel-body">
                                    <div class="table-responsive">
                                        <table class="table" style="font-size:12px;">
                                            <thead>
                                                <tr>
                                                    <th>Employee's Name </th>
                                                    <th>Gender</th>
                                                    <th>Phone number</th>
                                                    <th>Company</th>
                                                    <th>Department</th>
                                                    <th>Account status</th>
                                                    <th>Picture</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="employed in tabSearchEmployed" ng-click="selectedEmployed(employed)" style="cursor:pointer;">
                                                    <td>{{employed.name}}</td>
