                        var year=data.datenaiss.toString().split('/')[2];
                        var now=new Date().getYear();
                        told.value=30;

                    }
                    picture.src="/images/"+data.picture;
                    document.querySelector('#modal').click();
                    console.log(data)
                }

</script>

