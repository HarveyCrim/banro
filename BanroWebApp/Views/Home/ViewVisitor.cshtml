@model BanroWebApp.Models.t_beneficiaires
@{
    BanroWebApp.Models.Authenticate auth = null;
    ViewBag.Title = "ViewEmployed";
    if (Session["userinfo"]!=null)
	{
		auth=(BanroWebApp.Models.Authenticate)Session["userinfo"];


    }
    else
    {
        Response.Redirect("~/logout");
    }
}


<div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            Add a new Visitor
            <a class="btn btn-primary" style="float:right;margin-top:-0.5em;" href="viewvisitors"><i class="fa fa-search"></i>Search</a>
     
             <button class="btn btn-success" style="float:right;margin-top:-0.5em;margin-left:15em;" type="button" ng-click="addVisitor()"><i class="fa fa-plus"></i>Register</button>
            
        </div>
        <div class="alert alert-success" id="alertSucc" style="display:none;">
            Successfull registration
        </div>
        <div class="panel-body">
            <div class="row">
                <img id="pict_visitor" src="data:image/png;base64," alt="Alternate Text" style="width:150px;height:150px;border:1px solid silver;" />
                <div style="display:inline-block;margin-top:1em;margin-left:1em;">
                    @*<button type="button" class="btn btn-primary btn-circle btn-lg">
                        <i class="fa fa-camera"></i>
                    </button>*@

                    <button type="button" ng-click="browserPictureVisitor()" class="btn btn-success btn-circle btn-lg">
                        <i class="fa fa-image"></i>
                    </button>

                </div>
                <hr />
                <div class="col-lg-6">



                    <div class="form-group">
                        <label>Name</label>
                        @Html.TextBoxFor(Model => Model.C_name, new { Class = "form-control", data_ng_model = "nameVisitor" })
                    </div>
                    <div class="form-group">
                        <label>Phone</label>
                        @Html.TextBoxFor(Model => Model.C_name, new { Class = "form-control", data_ng_model = "phoneVisitor" })
                    </div>
                    <div class="form-group">
                        <label>Gender</label>
                        @Html.DropDownListFor(Model => Model.C_sex, (IEnumerable<SelectListItem>)ViewBag.Sex, new { Class = "form-control", data_ng_model = "sexVisitor" })
                    </div>
                    <div class="form-group">
                        <label>Visito's Company</label>
                        @Html.TextBoxFor(Model => Model.C_name, new { Class = "form-control", data_ng_model = "companyVisitor" })
                    </div>
                    <div class="form-group">
                        <label>Cause</label>
                        @Html.TextAreaFor(Model => Model.C_motif_visit, new { Class = "form-control", data_ng_model = "causeVisitor",placeholder="The reason of his/her presence" })
                    </div>
                  
                <div class="form-group" style="display:none;">
                    <label>Succursale :</label>
                    <input type="text" name="name" value="@auth.Succursale" class="form-control" id="idVisitor" />
                </div>

                </div>
            </div>
            <!-- /.row (nested) -->
        </div>
        <!-- /.panel-body -->
    </div>
    <!-- /.panel -->
</div>

<div class="modal fade" id="myModalChild" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header" style="background-color: #337ab7;color:white;">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="myModalLabel">Ajouter enfant</h4>
            </div>

            <div class="modal-body">
                <form role="form" method="post">
                    <img id="picture" src="data:image/png;base64," alt="Alternate Text" style="width:150px;height:150px;border:1px solid silver;" />
                    <div style="display:inline-block;margin-top:1em;margin-left:1em;">
                        <button type="button" class="btn btn-primary btn-circle btn-lg">
                            <i class="fa fa-camera"></i>
                        </button>

                        <button type="button" ng-click="browserPicture()" class="btn btn-success btn-circle btn-lg">
                            <i class="fa fa-image"></i>
                        </button>

                    </div>
                    <hr />
                    <div class="form-group">
                        <label>Nom Enfant</label>
                        @Html.TextBoxFor(Model => Model.C_id, new { Class = "form-control", @Value = @ViewBag.Id_Succ, @Id = "id_succ" })
                    </div>
                    <div class="form-group">
                        <label>Sexe</label>
                        @Html.DropDownListFor(Model => Model.C_sex, (IEnumerable<SelectListItem>)ViewBag.Sex, new { Class = "form-control" })

                    </div>






                </form>
            </div>
            <div class="modal-footer">
                <div class="alert alert-success" id="alertSucc" style="display:none;float:left;width:70px;background-color:white;">
                    Modification éffectuée avec succès!
                </div>
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" ng-click="ModificationSuccursale()" class="btn btn-primary">Modifier Succursale</button>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<!-- /.modal -->
