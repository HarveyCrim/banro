@model BanroWebApp.Models.exportDataMCI
@{
    ViewBag.Title = "MedicalCrusherIssued";
    Layout = "~/Views/Shared/_Menu.cshtml";
    BanroWebApp.Models.Authenticate auth = null;
    if (Session["userinfo"] != null)
    {
        auth = (BanroWebApp.Models.Authenticate)Session["userinfo"];
    }
    else
    {
        Response.Redirect("~/");
    }
}
<div id="page-wrapper" >
    <div class="row">
        <div class="col-lg-12">
            <h3 class="page-header">
                Medical Vouchers Issued <i class="fa fa-hospital-o" style="float:right;"></i>
            </h3>
        </div>
        <!-- /.col-lg-12 -->
    </div>


    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Reporting System /  Medical Vouchers Issued
                </div>

                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-6">
                           
                            <form method="post" >
                                <div data-ng-app="app" ng-controller="CtrlHome">
                                    
                                    @using (Html.BeginForm())
                                    {
                                        <div class="form-group">
                                            <label>Timing</label>
                                            @Html.DropDownListFor(Model => Model.category, (IEnumerable<SelectListItem>)ViewBag.CategoryMVI, new { Class = "form-control", @data_ng_model = "categoryExport", @data_ng_change = "categCompany()" })
                                            @*<select class="form-control" ng-change="categCompany()" ng-model="categoryExport">
                        <option value="Monthly">Monthly</option>
                        <option value="Weekly">Weekly</option>
                        <option value="Daily">Daily</option>
                        <option value="Choose">Choose date</option>
                    </select>*@

                                        </div>
                                        <div class="form-group" ng-show="dategroup">
                                            <label>Starting Date</label>
                                            <div class="input-group date dp" style="" data-provider="datepicker">
                                                @Html.TextBoxFor(Model => Model.date1, new { Class = "form-control", placeholder = "From", @Id = "date1Export" })
                                                @* <input id="date1Export" style="" placeholder="From" type="text" name="name" class="form-control" />*@
                                                <div class="input-group-addon">
                                                    <span class="fa fa-th"></span>
                                                </div>
                                            </div>

                                        </div>
                                        <div class="form-group" ng-show="dategroup">
                                            <label>Ending Date</label>
                                            <div class="input-group date dp" style="" data-provider="datepicker">
                                                @Html.TextBoxFor(Model => Model.date2, new { Class = "form-control", placeholder = "To", @Id = "date2Export" })
                                                @*  <input id="date2Export" style="" type="text" placeholder="To" name="name" class="form-control" />*@
                                                <div class="input-group-addon">
                                                    <span class="fa fa-th"></span>
                                                </div>
                                            </div>
                                        </div>

                                       


                                        <div style="width:100%;text-align:left;">


                                              <button type="button" class="btn btn-success" ng-click="exportMCI()"><i class="fa fa-file-excel-o"></i> Generate File</button>
                                            <button type="submit" style="display:none;" id="btnSendMVI" class="btn btn-success">Generate File <i class="fa fa-file-excel-o"></i></button>
                                        </div>
                                    }
                                </div>

                            </form>
                        </div>
                        <!-- /.col-lg-6 (nested) -->
                        <div class="col-lg-6">
                            @* <img src="~/Content/img/user2-160x160.jpg" alt="Alternate Text" style="width:260px;height:180px;" />*@
                        </div>
                        <!-- /.col-lg-6 (nested) -->
                    </div>
                    <!-- /.row (nested) -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
</div>
<!-- /#page-wrapper -->


